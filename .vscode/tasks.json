{
    "label": "build",
    "type": "shell",
    "command": "cmake",
    "args": [
        "--build", "${workspaceFolder}/build"
    ],
    "dependsOn": ["configure"],
    "group": {
        "kind": "build",
        "isDefault": true
    },
    "options": {
        "shell": {
            "executable": "cmd.exe",
            "args": ["/d", "/c"]
        }
    },
    "problemMatcher": ["$gcc"]
}